---

id: hg19/gnomAD_exome

resource_type: score_resources.AlleleScoreResource

filename: gnomad.exomes.r2.1.sites.tsv.gz
format: tsv/bgz

index_file:
  filename: gnomad.exomes.r2.1.sites.tsv.gz.tbi
  format: tabix

has_header: true

# defined by score_type
chrom: 
  name: chr
pos_begin:
  name: position
variant:
  name: CSHL_variant

# score values
scores:
    - id: AC
      type: int
      name: AC

    - id: AN
      type: int
      name: AN

    - id: AF
      type: float
      name: AF

    - id: AF_percent
      type: float
      name: AF_percent

    - id: controls_AC
      type: int
      name: controls_AC

    - id: controls_AN
      type: int
      name: controls_AN

    - id: controls_AF
      type: float
      name: controls_AF

    - id: controls_AF_percent
      type: float
      name: controls_AF_percent

    - id: non_neuro_AC
      type: int
      name: non_neuro_AC

    - id: non_neuro_AN
      type: int
      name: non_neuro_AN

    - id: non_neuro_AF
      type: float
      name: non_neuro_AF

    - id: non_neuro_AF_percent
      type: float
      name: non_neuro_AF_percent

default_annotation:
  attributes:
    - source: AC
      dest: exome_gnomad_ac
    - source: AN
      dest: exome_gnomad_an
    - source: AF
      dest: exome_gnomad_af
    - source: controls_AC
      dest: exome_gnomad_controls_ac
    - source: controls_AN
      dest: exome_gnomad_controls_an
    - source: controls_AF
      dest: exome_gnomad_controls_af
    - source: non_neuro_AC
      dest: exome_gnomad_non_neuro_ac
    - source: non_neuro_AN
      dest: exome_gnomad_non_neuro_an
    - source: non_neuro_AF
      dest: exome_gnomad_non_neuro_af
    - source: AF_percent
      dest: exome_gnomad_af_percent
    - source: controls_AF_percent
      dest: exome_gnomad_controls_af_percent
    - source: non_neuro_AF_percent
      dest: exome_gnomad_non_neuro_af_percent
